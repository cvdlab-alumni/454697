//ES 1
var d1 = DOMAIN ([[1, 5]])([4]);
var d2 = DOMAIN ([[1,5], [1, 3]])([4, 2]);
var d3 = DOMAIN ([[1,5], [1, 3], [2, 3]])([4, 2, 1]);

var domain = DOMAIN ([[0,10]]) ([10]);
var mapping = function (p){
	var u = p[0];
	return [u, 1];
}
var mapped = MAP (mapping)(domain);
//ES 2
var mappingBisettrice = function (p){
  var u = p[0];
	return [u, u];
}
var mapped2 = MAP (mappingBisettrice)(domain);
